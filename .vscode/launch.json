    "version": "0.2.0",
    "configurations": [

        {
            "name": "Run VGG Fit Script",
            "type": "debugpy",
            "request": "launch",
            "program": "src/cli.py",
            "console": "integratedTerminal",
            "args": [
                "fit",
                "--config","run_configs/fit_vgg_config.yaml",
            ],
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "env": {
                "PATH": "$PATH:$(pwd)/src"
            }
        },
        {
            "name": "Run UNet Fit Script",
